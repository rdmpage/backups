<beast>
	
	<taxa id="taxa">
		<taxon id="1Nectarinia_sovimanga_sovimang"/>
		<taxon id="2Nectarinia_sovimanga_sovimang"/>
		<taxon id="3Nectarinia_sovimanga_sovimang"/>
		<taxon id="4Nectarinia_sovimanga_sovimang"/>
		<taxon id="Nectarinia_sovimanga_abbotti"/>
		<taxon id="1Nectarinia_sovimanga_aldabren"/>
		<taxon id="2Nectarinia_sovimanga_aldabren"/>
		<taxon id="3Nectarinia_sovimanga_aldabren"/>
		<taxon id="1Nectarinia_humbloti_humbloti"/>
		<taxon id="Nectarinia_humbloti_mohelica"/>
		<taxon id="1Nectarinia_humbloti_mohelica"/>
		<taxon id="2Nectarinia_humbloti_mohelica"/>
	</taxa>

	<taxa id="humbloti">
		<taxon idref="1Nectarinia_humbloti_humbloti"/>
		<taxon idref="Nectarinia_humbloti_mohelica"/>
		<taxon idref="1Nectarinia_humbloti_mohelica"/>
		<taxon idref="2Nectarinia_humbloti_mohelica"/>
	</taxa>
	
	<alignment dataType="nucleotide" id="alignment">
		<sequence>
			<taxon idref="1Nectarinia_sovimanga_sovimang"/>
			
		</sequence>
		<sequence>
			<taxon idref="2Nectarinia_sovimanga_sovimang"/>
			
		</sequence>
		<sequence>
			<taxon idref="3Nectarinia_sovimanga_sovimang"/>
			
		</sequence>
		<sequence>
			<taxon idref="4Nectarinia_sovimanga_sovimang"/>
			
		</sequence>
		<sequence>
			<taxon idref="Nectarinia_sovimanga_abbotti"/>
			
		</sequence>
		<sequence>
			<taxon idref="1Nectarinia_sovimanga_aldabren"/>
			
		</sequence>
		<sequence>
			<taxon idref="2Nectarinia_sovimanga_aldabren"/>
			
		</sequence>
		<sequence>
			<taxon idref="3Nectarinia_sovimanga_aldabren"/>
			
		</sequence>
		<sequence>
			<taxon idref="1Nectarinia_humbloti_humbloti"/>
			
		</sequence>
		<sequence>
			<taxon idref="Nectarinia_humbloti_mohelica"/>
			
		</sequence>
		<sequence>
			<taxon idref="1Nectarinia_humbloti_mohelica"/>
			---------------------------
		</sequence>
		<sequence>
			<taxon idref="2Nectarinia_humbloti_mohelica"/>
			
		</sequence>
	</alignment>
	
	<patterns id="patterns_atp6" from="1" to="588">
		<alignment idref="alignment"/>
	</patterns>

	<patterns id="patterns_nd4" from="589" to="936">
		<alignment idref="alignment"/>
	</patterns>

	<patterns id="patterns_cytb" from="937" to="1220">
		<alignment idref="alignment"/>
	</patterns>
	
	<constantSize id="constant" units="substitutions">
		<populationSize>
			<parameter id="constant.popSize" value="100.0" lower="0.0"/>
		</populationSize>
	</constantSize>
	
	<!-- Generate a random starting tree under the coalescent process            -->
	<coalescentTree id="startingTree" rootHeight="1000000">
		<taxa idref="taxa"/>
		<constantSize idref="constant"/>
	</coalescentTree>
	
	<treeModel id="treeModel">
		<coalescentTree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>
	
	<coalescentLikelihood id="coalescent">
		<model>
			<constantSize idref="constant"/>
		</model>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
	</coalescentLikelihood>
	
	<gtrModel id="gtr_atp6">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment"/>
				<frequencies>
					<parameter id="gtr_atp6.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rateAC>
			<parameter id="gtr_atp6.ac" value="1" lower="0" upper="500"/>
		</rateAC>
		<rateAG>
			<parameter id="gtr_atp6.ag" value="1" lower="0" upper="500"/>
		</rateAG>
		<rateAT>
			<parameter id="gtr_atp6.at" value="1" lower="0" upper="500"/>
		</rateAT>
		<rateCG>
			<parameter id="gtr_atp6.cg" value="1" lower="0" upper="500"/>
		</rateCG>
		<rateCT>
			<parameter id="gtr_atp6.ct" value="1" lower="0" upper="500"/>
		</rateCT>
	</gtrModel>
	
	<gtrModel id="gtr_nd4">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment"/>
				<frequencies>
					<parameter id="gtr_nd4.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<rateAC>
			<parameter id="gtr_nd4.ac" value="1" lower="0" upper="500"/>
		</rateAC>
		<rateAG>
			<parameter id="gtr_nd4.ag" value="1" lower="0" upper="500"/>
		</rateAG>
		<rateAT>
			<parameter id="gtr_nd4.at" value="1" lower="0" upper="500"/>
		</rateAT>
		<rateCG>
			<parameter idref="gtr_nd4.at" value="1" lower="0" upper="500"/>
		</rateCG>
		<rateCT>
			<parameter idref="gtr_nd4.ag" value="1" lower="0" upper="500"/>
		</rateCT>
	</gtrModel>

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<hkyModel id="hky_cytb">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment"/>
				<frequencies>
					<parameter id="hky_cytb.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="hky_cytb.kappa" value="25" lower="0" upper="500"/>
		</kappa>
	</hkyModel>
	
	<!-- site model                                                              -->
	<siteModel id="siteModel_atp6">
		<substitutionModel>
			<gtrModel idref="gtr_atp6"/>
		</substitutionModel>
		<mutationRate>
			<parameter id="siteModel_atp6.mu" value="1.0" lower="0.0"/>
		</mutationRate>
		<gammaShape gammaCategories="6">
			<parameter id="siteModel_atp6.alpha" value="0.5" lower="0" upper="500"/>
		</gammaShape>
	</siteModel>

	<!-- site model                                                              -->
	<siteModel id="siteModel_nd4">
		<substitutionModel>
			<gtrModel idref="gtr_nd4"/>
		</substitutionModel>
		<mutationRate>
			<parameter id="siteModel_nd4.mu" value="1.0" lower="0.0"/>
		</mutationRate>
	</siteModel>

	<!-- site model                                                              -->
	<siteModel id="siteModel_cytb">
		<substitutionModel>
			<hkyModel idref="hky_cytb"/>
		</substitutionModel>
		<mutationRate>
			<parameter id="siteModel_cytb.mu" value="1.0" lower="0.0"/>
		</mutationRate>
	</siteModel>

	<tmrcaStatistic id="humbloti-split" name="humbloti-split">
		<mrca>
			<taxa idref="humbloti"></taxa>
		</mrca>
		<treeModel idref="treeModel"></treeModel>
	</tmrcaStatistic>

	<distributionLikelihood id="calibration">
		<distribution>
			<normalDistributionModel>
 				<mean> 500000 </mean>
				<stdev> 100 </stdev>
			</normalDistributionModel>
		</distribution>
		<data>
			<parameter idref="humbloti-split"/>
		</data>
	</distributionLikelihood>

	<discretizedBranchRates id="discreteBranchRates">
        <treeModel idref="treeModel"/>
        <distribution>
            <logNormalDistributionModel id="lnd" meanInRealSpace="true">
                <mean> <parameter id="lndMean" value="1e-8" lower="0" upper="1.0"/> </mean>
                <stdev> <parameter id="lndStDev" value="1e-8" lower="0.0" upper="5.0"/> </stdev>
            </logNormalDistributionModel>
        </distribution>
        <rateCategories> <parameter id="rateCategories" dimension="22"/> </rateCategories>
    </discretizedBranchRates>

	<compoundParameter id="allMus">
		<parameter idref="siteModel_atp6.mu"/>
		<parameter idref="siteModel_nd4.mu"/>
		<parameter idref="siteModel_cytb.mu"/>
	</compoundParameter>
	
	<treeLikelihood id="treeLikelihood1">
		<discretizedBranchRates idref="discreteBranchRates"/>
		<patterns idref="patterns_atp6"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel_atp6"/>
	</treeLikelihood>

	<treeLikelihood id="treeLikelihood2">
		<discretizedBranchRates idref="discreteBranchRates"/>
		<patterns idref="patterns_nd4"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel_nd4"/>
	</treeLikelihood>

	<treeLikelihood id="treeLikelihood3">
		<discretizedBranchRates idref="discreteBranchRates"/>
		<patterns idref="patterns_cytb"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel_cytb"/>
	</treeLikelihood>
	
	<rateStatistic name="meanRate" id="meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<branchRateModel idref="discreteBranchRates"/>
	</rateStatistic>

	<rateStatistic name="rateCoeff" id="rateCoeff" mode="coefficientOfVariation" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<branchRateModel idref="discreteBranchRates"/>
	</rateStatistic>

	<operators id="operators">
		<scaleOperator scaleFactor="0.4305" weight="1" adapt="false">
			<parameter idref="gtr_atp6.ag"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.1853" weight="1" adapt="false">
			<parameter idref="gtr_atp6.ct"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.5" weight="1">
			<parameter idref="siteModel_atp6.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.4305" weight="1" adapt="false">
			<parameter idref="gtr_nd4.ag"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.1853" weight="1" adapt="false">
			<parameter idref="gtr_nd4.at"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.5" weight="1">
			<parameter idref="hky_cytb.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.5" weight="1">
			<parameter idref="constant.popSize"/>
		</scaleOperator>
		<centeredScale scaleFactor="0.75" weight="1">
			<parameter idref="allMus"/>
		</centeredScale>
		<deltaExchange delta="0.75" weight="1">
			<parameter idref="allMus"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.9" adapt="false" weight="1">
			<parameter idref="lndMean"/>
		</scaleOperator>
		<swapOperator autoOptimize="false" weight="5" size="1">
			<parameter idref="rateCategories"/>
		</swapOperator>
		<upDownOperator weight="4" scaleFactor="0.9">
			<up>
				<parameter idref="lndMean"/>
			</up>
			<down>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</down>
		</upDownOperator>
		<scaleOperator scaleFactor="0.5" weight="1">
			<parameter idref="lndStDev"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.5" weight="1">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="10">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
		<subtreeSlide weight="5" gaussian="true" size="1.0">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="1">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="1">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="1">
			<treeModel idref="treeModel"/>
			<constantSize idref="constant"/>
		</wilsonBalding>
	</operators>
	
	<mcmc id="mcmc" chainLength="10000000" autoOptimize="true">
		<compoundLikelihood id="likelihood">
			<coalescentLikelihood idref="coalescent"/>
			<treeLikelihood idref="treeLikelihood1"/>
			<treeLikelihood idref="treeLikelihood2"/>
			<treeLikelihood idref="treeLikelihood3"/>
			<distributionLikelihood idref="calibration"/>
		</compoundLikelihood>
		<operators idref="operators"/>
		<log id="screenLog" logEvery="1000">
			<column label="Likelihood" dp="4" width="12">
				<compoundLikelihood idref="likelihood"/>
			</column>
			<column label="Root Height" sf="6" width="12">
				<parameter idref="treeModel.rootHeight"/>
			</column>
			<column label="Mean Rate" sf="6" width="12">
				<parameter idref="meanRate"/>
			</column>
		</log>
		<log id="fileLog" logEvery="1000" fileName="sunbirds3.log">
			<compoundLikelihood idref="likelihood"/>
			<parameter idref="constant.popSize"/>
			<parameter idref="gtr_atp6.ag"/>
			<parameter idref="gtr_atp6.ct"/>
			<parameter idref="siteModel_atp6.alpha"/>
			<parameter idref="gtr_nd4.ag"/>
			<parameter idref="gtr_nd4.at"/>
			<parameter idref="hky_cytb.kappa"/>
			<rateStatistic idref="meanRate"/>
			<rateStatistic idref="rateCoeff"/>
			<parameter idref="treeModel.rootHeight"/>
			<treeLikelihood idref="treeLikelihood1"/>
			<treeLikelihood idref="treeLikelihood2"/>
			<treeLikelihood idref="treeLikelihood3"/>
			<coalescentLikelihood idref="coalescent"/>
		</log>
	</mcmc>
	
	<report>
		<property name="timer">
			<object idref="mcmc"/>
		</property>
	</report>
	
</beast>
